{
    "name": "milo-widgets",
    "version": "1.0.0",
    "description": "Embedable widgets to search Milo data",
    "scripts": {
        "ng": "ng",
        "start": "ng serve -p 9010",
        "build": "webpack -p --progress && ng build -prod -e prod -pr false -aot false",
        "deploy": "npm run build && firebase deploy",
        "deploy-configurations": "node ./configurations/compile-configurations.js && firebase database:set -y /configurations ./configurations/configurations.json",
        "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
        "test": "ng test",
        "pree2e": "webdriver-manager update --standalone false --gecko false",
        "e2e": "protractor"
    },
    "keywords": [
        "SCVO",
        "Milo"
    ],
    "contributors": [
        "Matthew Wilkes <matthew.wilkes@scvo.org.uk>",
        "Calum Mac√ôisdean <calum.macuisdean@scvo.org.uk>"
    ],
    "license": "MIT",
    "dependencies": {
        "@angular-mdl/core": "^4.0.8",
        "@angular-mdl/datepicker": "^0.0.3",
        "@angular-mdl/popover": "^0.6.4",
        "@angular-mdl/select": "^0.10.5",
        "@angular/animations": "^4.3.2",
        "@angular/common": "^4.3.2",
        "@angular/compiler": "^4.3.2",
        "@angular/core": "^4.3.2",
        "@angular/forms": "^4.3.2",
        "@angular/http": "^4.3.2",
        "@angular/platform-browser": "4.3.2",
        "@angular/platform-browser-dynamic": "4.3.2",
        "@angular/router": "4.3.2",
        "@types/elasticsearch": "^5.0.14",
        "@types/google-maps": "^3.2.0",
        "@types/handlebars": "^4.0.35",
        "@types/jquery": "^3.2.10",
        "angularfire2": "^4.0.0-rc.1",
        "firebase": "^4.2.0",
        "node-sass": "^4.5.3",
        "readline": "^1.3.0",
        "rxjs": "5.4.2",
        "zone.js": "0.8.16"
    },
    "devDependencies": {
        "@angular/cli": "^1.2.6",
        "@angular/compiler-cli": "4.3.2",
        "@types/core-js": "^0.9.42",
        "@types/hammerjs": "^2.0.34",
        "@types/jasmine": "^2.5.53",
        "@types/node": "^8.0.17",
        "bootstrap-sass": "^3.3.7",
        "core-js": "^2.4.1",
        "elasticsearch": "git+https://git@github.com/scvodigital/elasticsearch-js.git",
