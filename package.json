        "handlebars": "^4.0.6",
        "handlebars-helpers": "^0.8.2",
        "jquery": "^3.2.1",
        "string": "^3.3.3",
        "typescript": "^2.3.4",
        "unlazy-loader": "^0.1.2",
        "webpack": "2.3.3",
        "extract-text-webpack-plugin": "2.1.0",
        "webpack-bundle-analyzer": "2.8.2",
        "webpack-dev-server": "2.4.1",
        "awesome-typescript-loader": "^3.2.2",
        "compression-webpack-plugin": "^1.0.0",
        "copy-webpack-plugin": "^4.0.1",
        "css-loader": "^0.28.4",
